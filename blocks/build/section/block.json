{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "lichtberg/section",
  "version": "0.5.0",
  "title": "Section",
  "category": "lichtberg",
  "icon": "grid-view",
  "description": "A section block to encapsulate content.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "lichtberg",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "classBaseName": {
      "type": "string",
      "default": "wp-block-lichtberg-section"
    },
    "style": {
      "type": "array",
      "default": []
    },
    "styleOptions": {
      "type": "array",
      "default": []
    }
  }
}