{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "lichtberg/image",
  "ancestor": [
    "lichtberg/group"
  ],
  "version": "0.5.0",
  "title": "Image",
  "category": "lichtberg",
  "icon": "format-image",
  "description": "Image block to display media.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "lichtberg",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "alt": {
      "type": "string",
      "default": ""
    },
    "caption": {
      "type": "string",
      "default": ""
    },
    "classBaseName": {
      "type": "string",
      "default": "wp-block-lichtberg-image"
    },
    "id": {
      "type": "number",
      "default": 0
    },
    "style": {
      "type": "array",
      "default": []
    },
    "styleOptions": {
      "type": "array",
      "default": []
    },
    "url": {
      "type": "string",
      "default": ""
    }
  }
}