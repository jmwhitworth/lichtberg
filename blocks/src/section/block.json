{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "lichtberg/section",
	"version": "0.1.0",
	"title": "Section",
	"category": "lichtberg",
	"icon": "grid-view",
	"description": "A section block to encapsulate content.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "lichtberg",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"style": {
			"type": "array",
			"default": []
		},
		"options": {
			"type": "object",
			"default": {
				"style": [],
				"classBaseName": "wp-block-lichtberg-section"
			}
		}
	}
}
